/*tooltip para btn configurar consulta*/
#btnConfigurarGrafica {outline:none; }
#btnConfigurarGrafica strong {line-height:30px;}
#btnConfigurarGrafica:hover {text-decoration:none;} 
#btnConfigurarGrafica span {
    z-index:999;display:none; font-size: 12px; 
    padding:2px 6px;
    margin-top:4px; margin-left:55px;
    width:150px; line-height:16px;
}
#btnConfigurarGrafica:hover span{
    display:inline; position:absolute; color:#000000;
    top:20px; left:20px; 
    background:#ABABAB;}
.callout {z-index:2;position:relative;top:0px;border:0;left:0px;}
    
/*tooltip informacion entes*/
