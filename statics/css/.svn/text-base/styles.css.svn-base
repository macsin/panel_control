/* leyenda en boton accion para usuario dependencia,  */
a#Ntooltip,a#Ntooltip1,a#Ntooltip2,a#Ntooltip3,a#Ntooltip4,a#Ntooltip5,a#Ntooltip6,td#Ntooltip1,div#Ntooltip2 {
position: relative; 
text-decoration: none !important; 
}

a#Ntooltip:hover,a#Ntooltip1:hover,a#Ntooltip2:hover
,a#Ntooltip3:hover,a#Ntooltip4:hover,a#Ntooltip5:hover,a#Ntooltip6:hover,td#Ntooltip1:hover,div#Ntooltip2:hover {
z-index:999;
}

a#Ntooltip span.accion,a#Ntooltip1 span.accion,a#Ntooltip2 span.accion, 
a#Ntooltip3 span.accion,a#Ntooltip4 span.accion,a#Ntooltip5 span.accion,a#Ntooltip6 span.accion,td#Ntooltip1 span.accion, div#Ntooltip2 span.accion {
display: none;
}

a#Ntooltip:hover span.accion,a#Ntooltip1:hover span.accion,a#Ntooltip2:hover span.accion,
a#Ntooltip3:hover span.accion ,a#Ntooltip4:hover span.accion,a#Ntooltip5:hover span.accion,a#Ntooltip6:hover span.accion,
 td#Ntooltip1:hover span.accion, div#Ntooltip2:hover span.accion{
display: block;
position: absolute; 
top:2em; left:2em; 
width:160px; 
padding:1px; 
background-color: #CCCCCC; 
color: #000000;
}
