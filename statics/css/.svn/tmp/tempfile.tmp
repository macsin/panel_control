/* leyenda en boton accion para usuario dependencia */
a#Ntooltip {
position: relative; 
text-decoration: none !important; 
}

a#Ntooltip:hover {
z-index:999;
}

a#Ntooltip span.accion {
display: none;
}

a#Ntooltip:hover span.accion {
display: block;
position: absolute; 
top:2em; left:2em; 
width:150px; 
padding:1px; 
background-color: #CCCCCC; 
color: #000000;
}
